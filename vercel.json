{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node@2.15.10"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "framework": "node",
  "nodeVersion": "18.x"
} 